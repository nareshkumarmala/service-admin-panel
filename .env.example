# WayPartner Admin Panel - Environment Variables (INTERNAL USE ONLY)

# Supabase Configuration (Admin Access)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Admin Panel Configuration
VITE_INTERNAL_USE_ONLY=true
VITE_ADMIN_PANEL_MODE=true
VITE_APP_NAME="WayPartner Admin Panel"
VITE_APP_VERSION="2.1.0"

# Error Tracking (Optional)
VITE_SENTRY_DSN=your_sentry_dsn
VITE_LOG_ROCKET_APP_ID=your_logrocket_id

# Security Configuration
VITE_ADMIN_SESSION_TIMEOUT=3600000
VITE_REQUIRE_2FA=false
VITE_ALLOWED_IPS=127.0.0.1,your_office_ip

# Feature Flags (Admin)
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_SYSTEM_HEALTH=true

# API Configuration (Admin)
VITE_API_BASE_URL="https://your-admin-api.com"
VITE_ADMIN_WEBHOOK_URL="https://your-admin-webhook.com"